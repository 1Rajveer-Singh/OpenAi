{
  "version": 2,
  "builds": [
    {
      "src": "react-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "react-frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "react-frontend/build/static/$1"
    },
    {
      "src": "/favicon.svg",
      "dest": "react-frontend/build/favicon.svg"
    },
    {
      "src": "/manifest.json",
      "dest": "react-frontend/build/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "react-frontend/build/index.html"
    }
  ]
}